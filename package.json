{
	"name": "vscode-designer-addon",
	"description": "A HTML Designer for VsCode",
	"version": "0.0.1",
	"publisher": "node-projects",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Programming Languages",
		"Visualization"
	],
	"activationEvents": [
		"onWebviewPanel:designer"
	],
	"main": "./out/vscode/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "designer.designerTextEditor",
				"displayName": "Designer",
				"selector": [
					{
						"filenamePattern": "*.html"
					},
					{
						"filenamePattern": "*.vue"
					}
				],
				"priority": "option"
			}
		],
		"views": {
			"explorer": [
				{
					"id": "exampleView",
					"name": "exampleViewAAAA"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -w -p ./src/vscode",
		"watchWebview": "tsc -w -p ./src/webview"
	},
	"devDependencies": {
		"@types/node": "^18.11.14",
		"@types/vscode": "^1.74.0",
		"@types/vscode-webview": "^1.57.0",
		"@typescript-eslint/eslint-plugin": "^5.46.1",
		"@typescript-eslint/parser": "^5.46.1",
		"eslint": "^8.29.0",
		"typescript": "^4.9.4"
	},
	"dependencies": {
		"@node-projects/base-custom-webcomponent": "^0.10.7",
		"@node-projects/lean-he-esm": "^3.3.0",
		"@node-projects/node-html-parser-esm": "^2.5.1",
		"@node-projects/web-component-designer": "^0.0.161",
		"construct-style-sheets-polyfill": "^3.1.0",
		"es-module-shims": "^1.6.2"
	}
}